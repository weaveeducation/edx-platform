<select name="copy-to-courses" multiple="multiple" class="copy-to-courses" style="width: 600px; text-overflow: ellipsis;">
    <% _.each(courses, function(course) { %>
    <option value="<%- course.id %>"><%- course.name %></option>
    <% }); %>
</select>
<div class="copy-to-course-result" style="padding-top: 10px;"></div>
