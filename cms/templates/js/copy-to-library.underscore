<select name="copy-to-library" class="copy-to-course" style="width: 600px; text-overflow: ellipsis;">
    <% _.each(libraries, function(library) { %>
    <option value="<%- library.id %>"><%- library.name %></option>
    <% }); %>
</select>
<div class="copy-to-course-result" style="padding-top: 10px;"></div>
